@unpublished{payton_critical_2024,
	location = {Bundesanstalt für Materialprüfung und Forschung ({BAM}), Berlin, Germany},
	title = {Critical Alloys for Sustainability: Conundrums, Challenges, and Creating a Path to the Future},
	abstract = {Hundreds of billions of dollars are currently being invested by many countries around the world to develop domestic manufacturing capabilities to achieve their clean energy and decarbonization goals. Electric vehicles, wind turbines, solar panels, microchips, and many carbon capture devices are dependent on minerals that are of limited availability worldwide. Many of these technologies are also steel and concrete intensive in their installations. Steel and concrete are counted among the most greatest decarbonization challenges the world faces. The adoption of hydrogen for industrial heating and reduction processes presents a risk to present steel-dominated transportation and storage infrastructure. Thus, humanity is faced with a conundrum: Many materials upon which we are dependent for addressing climate change are themselves substantial contributors to {CO}2 emissions. Shifting our economies from fuel-based to materials-based systems will require intensive mining of minerals for which the world’s supply is limited, as well as construction of new buildings, manufacturing capabilities, and infrastructure. Furthermore, the rapid introduction of different specialty alloy grades over the past 40 years (responsible for significant improvements in safety and performance) presents a challenge for the creation of sustainable circular economies. This talk will explore the geopolitical, national security, economic security, and climate implications of a ‘green gold rush’ on secondary processing of metallic materials. It will trace the supply chains of a few regionally important critical materials from where they are mined through their role in alloys critical for the energy and economic security. Tools and prospects for sustainable metallurgy will be presented, along with some select basic science gaps that could be collaboratively addressed to the benefit of the international economy, climate, and society.},
	type = {Materials Engineering Department Seminar},
	howpublished = {Materials Engineering Department Seminar},
	author = {Payton, E. J.},
	date = {2024-06-27},
}

@article{cao_panevolution_2025,
	title = {\{{PanEvolution}\}: Integrating {CALPHAD}, microstructure modeling, and finite element method},
	volume = {90},
	doi = {10.1016/j.calphad.2025.102851},
	abstract = {This paper introduces {PanEvolution}, a modeling platform that integrates {CALPHAD}, microstructure modeling, and finite element method ({FEM}). This work represents the third generation of Pandat software, designed to simulate the microstructure evolution of metallic materials during conventional manufacturing processes. The key features of {PanEvolution} include: (1) an integrated modeling framework that allows direct coupling of {CALPHAD} with various {FEM} software packages. This enables {FEM} packages to obtain accurate input of phase information for multi-component alloys and perform simulations of microstructure evolution for technically important metallic alloys, including precipitation, recrystallization, grain growth, and coarsening, during conventional manufacturing processes such as forging and rolling. (2) an open architecture that facilitates the easy replacement and swapping of fast-acting microstructure models.  This enables microstructure evolution and property models developed by academia or industry to be easily integrated into the {PanEvolution} platform to maximize their value in industrial applications.},
	pages = {102851},
	journaltitle = {{CALPHAD}},
	author = {Cao, Weisheng and Zhang, Fan and Kadirvel, Kamalnath and Chen, Shuanglin and Payton, Eric J. and Krug, Matthew E.},
	date = {2025-09-01},
}

