@article{lee_prevalence_2024,
	title = {Prevalence of heteroepitaxial recrystallization in the low solvus high refractory ({LSHR}) \${\textbackslash}gamma\$-\${\textbackslash}gamma{\textasciicircum}{\textbackslash}prime\$ superalloy},
	volume = {13},
	issn = {2192-9270},
	url = {https://doi.org/10.1007/s13632-023-01036-z},
	doi = {10.1007/s13632-023-01036-z},
	abstract = {Heteroepitaxial recrystallization ({HeRX}) is a recently discovered recrystallization mechanism that can occur during thermomechanical processing of Ni-base superalloys. It is important to understand this mechanism because it offers insights into grain size control, which is critical to the tensile, fatigue, and creep properties of superalloys. In this paper, {HeRX} activity in the low solvus high refractory ({LSHR}) is quantitatively characterized using combined energy-dispersive X-ray spectroscopy and electron backscatter diffraction. The formation of heteroepitaxially recrystallized grains is observed within the intermediate-misfit γ–γ′ superalloy {LSHR}; this suggests the {HeRX} is more widespread than initially thought. Microstructural imaging indicates that the presence of a coherent γ shell formed via inverse precipitation is not a necessary condition for the nucleation of {HeRX} grains. {HeRX} grains are found to comprise the large grain tail of the grain size distribution, regardless of processing condition. The extent of {HeRX} grain formation and growth in {LSHR} is determined as a function of strain, forging rate, and thermal history.},
	pages = {114--129},
	number = {1},
	journaltitle = {Metallography, Microstructure, and Analysis},
	shortjournal = {Metallogr. Microstruct. Anal.},
	author = {Lee, Y. and Hershkovitz, E. and Kim, H. and Wertz, K. N. and Payton, E. J. and Miller, V. M.},
	urldate = {2025-05-11},
	date = {2024-02-01},
	langid = {english},
	keywords = {Heteroepitaxial Recrystallization, Ni-Superalloys, Recrystallization},
}

