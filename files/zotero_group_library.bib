	title = {{SPG}-50: Phase field modelling of microtubes formation in {NiTi} shape memory alloys},
	url = {https://www.matscitech.org/MST24},
	abstract = {Phase field is an important approach for simulating microstructure evolution at the mesoscale. Within the domain of polycrystalline metals, vacancy transport exerts a clear influence on microstructural changes. The vacancy dynamics and pore nucleation in the Ni-Ti alloy system are explained by the sharp interface phase field model. This research aims to simulate and understand the formation and evolution of Kirkendall pores near the center of Ti-coated Ni wires during interdiffusion, resulting in the creation of {NiTi} shape memory alloy microtubes. The sharp interface model was implemented in multiphysics software and different boundary conditions were explored to gain a greater understanding of the pore nucleation process that leads to microtube formation. The role of various simulation conditions in producing agreements and discrepancies with experimental observations will be discussed, along with prospects for connecting {CALPHAD} mobility databases to multiphysics software to investigate other alloy systems.},
	type = {Poster presentation in the symposium "2024 Graduate Student Poster Contest"},
	howpublished = {Poster presentation in the symposium "2024 Graduate Student Poster Contest"},
	note = {{MS}\&T24: Materials Science \& Technology},
	author = {Oruganti, V.},
	editora = {Josyula, S. and Kumar, R. and Puente, A. E. Paz y and Payton, E. J.},
	editoratype = {collaborator},
	date = {2024-10-08},
}

@unpublished{payton_icme_2025,
	location = {Las Vegas, {NV}, {USA}},
	title = {{ICME} in chemical vapor deposition synthesis and characterization of 2D refractory metal carbides},
	abstract = {Top-down etching and exfoliation for synthesis of 2D refractory metal carbides (functionalized as {MXenes}) has been demonstrated for a large number of compositions. However, bottom-up routes including chemical vapor deposition ({CVD}) have been less explored. Many papers have demonstrated liquid-phase ({LP}) {CVD} of Mo2C with Cu as the typical catalyst. This talk will address challenges associated with designing {LP}-{CVD} bottom-up approaches for other {MnCn}-1 refractory metal carbides from a thermodynamic and reaction equilibria perspective, showing that available thermodynamic data from density functional theory calculations ({DFT}) can guide synthesis but have substantial discrepancies with experimental results. Observations will be presented for several possible liquid and refractory metal substrate combinations, using {CH}4 as a carbon source and H2 as a reducing gas. {DFT}-based simulations of Raman spectra and modeled X-ray diffraction patterns will be compared with experimental observations and thermodynamic predictions of {MnCn}-1 where M is V, Nb, or Mo.},
	type = {Invited oral presentation in the symposium "2D Materials – Preparation, Properties, Modeling \& Applications: Processing, Characterization, Modeling \& Applications I"},
	howpublished = {Invited oral presentation in the symposium "2D Materials – Preparation, Properties, Modeling \& Applications: Processing, Characterization, Modeling \& Applications I"},
	note = {The Minerals Metals and Materials Society 2025 Annual Meeting and Exhibition},
	author = {Payton, E. J.},
	editora = {Hasan, S. and Sultana, N.},
	editoratype = {collaborator},
	date = {2025-03-25},
}

@unpublished{cao_software_2025,
	location = {Las Vegas, {NV}},
	title = {A Software Platform Integrating Microstructure Modeling with Finite Element Method},
	url = {https://www.tms.org/TMS2025/},
	abstract = {A software platform named {PanEvolution} has been developed for the simulation of microstructure evolution in metallic materials. The key features of this platform include: (1) an integrated framework that allows direct coupling with various Finite Element Method ({FEM}) software packages for processing inputs; (2) a modular design that facilitates the easy replacement and swapping of fast-acting microstructure models; and (3) seamless integration with a thermodynamic calculation engine based on {CALPHAD} method to obtain accurate input of phase information for technically important multi-component alloys. This enables the simulation of microstructure evolution in metallic alloys, including precipitation, recrystallization, grain growth, and coarsening, during conventional manufacturing processes such as forging and rolling. A general description of this framework, along with simulated examples, will be presented.},
	type = {Invited oral presentation in the symposium "Thermodynamics and Kinetics of Alloys {III} — Alloy Design and Thermodynamic Modeling"},
	howpublished = {Invited oral presentation in the symposium "Thermodynamics and Kinetics of Alloys {III} — Alloy Design and Thermodynamic Modeling"},
	note = {The Minerals Metals and Materials Society 2025 Annual Meeting and Exhibition},
	author = {Zhang, Fan},
	editora = {Cao, Weisheng and Kadirvel, Kamalnath and Payton, Eric John and Krug, Matthew E.},
	editoratype = {collaborator},
